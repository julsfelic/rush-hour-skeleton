<h1>Url Statistics</h1>

<h2 id="max">Maximum Response Time</h2><h3 id="max-time"><%= @url.max_response_time %></h3>
<h2>Minimum Response Time</h2><h3><%= @url.min_response_time %></h3>
<h2>Response Times in Descending order</h2><h3><%= @url.ordered_response_times.join(", ") %></h3>
<h2>Average Response Time</h2><h3><%= @url.avg_response_time %></h3>
<h2>All Associated Verbs</h2><h3><%= @url.associated_verbs.join(", ") %></h3>
<h2>Top Three Referrers</h2><h3><%= @url.top_three_referrers.join(", ") %></h3>
<h2>Three Most Popular User Agents</h2><h3><%= @url.three_most_popular_user_agents %></h3>
