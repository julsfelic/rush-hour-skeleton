<%= erb :_navbar %>

<div class="container client-container">
  <h1>Event Index</h1>
  <ul>
    <% @client.payload_requests.where(client_id: @client.id).pluck(:event_name).uniq.each do |event| %>
      <li>
        <a href="/sources/<%= @client.identifier %>/events/<%= event %>"><%= event %></a>
      </li>
    <% end %>
  </ul>
</div>
